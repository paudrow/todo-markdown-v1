{
  "name": "todo-markdown",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "package": "turbo package",
    "publish": "turbo publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
