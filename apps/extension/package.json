{
  "name": "@todo-markdown/extension",
  "displayName": "todo-markdown",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "todo-markdown",
          "title": "Markdown Todos",
          "icon": "$(checklist)"
        }
      ]
    },
    "views": {
      "todo-markdown": [
        {
          "id": "todoList",
          "name": "Todo List"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "dev": "npm run watch",
    "build": "npm run compile",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.11.24",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.4",
    "@todo-markdown/types": "*",
    "@todo-markdown/utils": "*"
  }
}
